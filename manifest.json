{
  "name": "Vietlubu Github Notification",
  "description": "Vì sao Github lại không push notification?",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Github notification",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/github16.png",
    "32": "assets/github32.png",
    "48": "assets/github48.png",
    "128": "assets/github128.png"
  }
}
